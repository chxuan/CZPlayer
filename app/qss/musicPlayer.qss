/************************************QSlider******************************************/

QSlider#seekSlider::groove:horizontal 
{
	border-left: -8px solid;
	background: rgba(0,0,0,0);
	height: 6px;
	border-radius: 6px;
}

QSlider#seekSlider::sub-page:horizontal 
{
	background:url(:/images/progressBarSub.png);
	border: 0px solid #777;
	height: 6px;
	border-radius: 0px;
}

QSlider#seekSlider::add-page:horizontal
{
	background: rgba(0,0,0,0);
	border: 0px solid #777;
	height: 6px;
	border-radius: 0px;
}

QSlider#seekSlider::handle:horizontal 
{
	border: 0px solid #777;
	width: 28px;
	height: 30px;
	border-left:0px;
	margin-top: -12px;
	margin-bottom: -12px;
	border-radius: 4px;
}

QSlider#volumeSlider::groove:horizontal 
{
	border-left: -8px solid;
	background: rgba(0,0,0,0);
	height: 6px;
	border-radius: 6px;
}

QSlider#volumeSlider::sub-page:horizontal 
{
	background:url(:/images/volumeBarSub.png);
	border: 0px solid #777;
	height: 6px;
	border-radius: 0px;
}

QSlider#volumeSlider::add-page:horizontal
{
	background: rgba(0,0,0,0);
	border: 0px solid #777;
	height: 6px;
	border-radius: 0px;
}

QSlider#volumeSlider::handle:horizontal 
{
	border: 0px solid #777;
	width: 28px;
	height: 30px;
	border-left:0px;
	margin-top: -12px;
	margin-bottom: -12px;
	border-radius: 4px;
}

QSlider::handle:horizontal:!hover 
{
	border-image:url(:/images/seekBarPoint.png);
	border: 0px solid #444;
	border-radius: 4px;
}

QSlider::handle:horizontal:hover
{
	border-image:url(:/images/seekBarPoint2.png);
	border: 0px solid #444;
	border-radius: 4px;
}

QSlider::handle:horizontal:pressed 
{
	border-image:url(:/images/seekBarPoint3.png);
	border: 0px solid #444;
	border-radius: 4px;
}

QSlider::handle:horizontal:disabled 
{
	border-image:url(:/images/seekBarPoint.png);
	border: 0px solid #444;
	border-radius: 0px;
}

